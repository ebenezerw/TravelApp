<div class="well">
  <h2><%= @destination.place %></h2>
  <%= @destination.traveler_name %>
  <p>
      <%= @destination.social_link %>
      <p>
          Start:
          <%= @destination.start_date %>
      </p>
      End:
      <%= @destination.end_date %>
      <p>
          <%= @destination.intro %>
      <p>
        <h5><%= link_to "Edit Destination", edit_destination_path(@destination) %></h5><p>
          <%= link_to "Delete Destination", destination_path(@destination), method: :delete, data: {confirm: "Are you sure you want to delete this trip?"} %>
      </div>

      <%= render "interests/show" %>



    <!-- Destination(id: integer, place: string, traveler_name: string, social_link: string, start_date: date, end_date: date, intro: text, created_at: datetime, updated_at: datetime) -->
