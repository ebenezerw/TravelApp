

<% @destination.each do |destination| %>
<div class="well">
  <div class="destinationName"> <h2><%= link_to destination.place, destination_path(destination) %></h2></div>
    <h3><%= destination.traveler_name %></h3>
        <h3><%= destination.social_link %></h3>
            <h3>Start: <%= destination.start_date %></h3>
        <h3>End: <%= destination.end_date %></h3>
            <h3><%= destination.intro %></h3>

</div>
    <% end %>
